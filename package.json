{
  "name": "litedash",
  "version": "0.0.4",
  "description": "A lightweight version of lodash",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "browser": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "prepublishOnly": "rm -rf $PUBLISH_DIR && mkdir $PUBLISH_DIR && cp -r dist $PUBLISH_DIR/dist && cp README.md $PUBLISH_DIR && DIR=$PUBLISH_DIR node scripts/prepare.js",
    "build": "rollup -c",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cirbuk/litedash.git"
  },
  "author": "Jophin Joseph",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cirbuk/litedash/issues"
  },
  "homepage": "https://github.com/cirbuk/litedash#readme",
  "devDependencies": {
    "@babel/core": "7.5.5",
    "@babel/plugin-proposal-class-properties": "7.5.5",
    "@babel/plugin-proposal-function-bind": "7.2.0",
    "@babel/plugin-proposal-object-rest-spread": "7.5.5",
    "@babel/preset-env": "7.5.5",
    "@babel/preset-typescript": "7.3.3",
    "babel-jest": "24.8.0",
    "jest": "24.8.0",
    "rollup": "1.18.0",
    "rollup-plugin-babel": "4.3.3",
    "rollup-plugin-commonjs": "10.0.2",
    "rollup-plugin-node-resolve": "5.2.0",
    "rollup-plugin-terser": "5.1.1",
    "rollup-plugin-typescript": "1.0.1",
    "tslib": "1.10.0",
    "typescript": "3.5.3"
  },
  "dependencies": {}
}
